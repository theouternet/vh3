<h1>Sign Up</h1>


<% if @user.errors.any? %>
<div class="notice alert-warning">
  <p>There were some errors:</p>
  <br>
    <% @user.errors.full_messages.each do |message| %>
      <p><%= message %></p>
    <% end %>
</div>
<% end %>


<%= form_for @user do |f| %>

    <%= f.label :name %>
    <%= f.text_field :name %> 

    <%= f.label :email %>
    <%= f.text_field :email %> 

    <%= f.label :password %>
    <%= f.password_field :password %> 

    <%= f.submit %>

    <% end %>

    <%= login_with_google %>
